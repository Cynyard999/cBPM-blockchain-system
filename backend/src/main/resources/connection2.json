{
    "name": "fabric-ca",
    "version": "3.7",
    "client": {
        "organization": "MiddlemanOrg",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "3000"
                },
                "orderer": "3000"
            }
        }
    },
    "channels": {
        "cbpmchannel": {
            "orderers": [
                "orderer-cbpm"
            ],
            "peers": {
                "peer1-supplier": {
                    "endorsingPeer": false,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer2-supplier": {
                    "endorsingPeer": false,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer1-carrier": {
                    "endorsingPeer": false,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer2-carrier": {
                    "endorsingPeer": false,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer1-manufacturer": {
                    "endorsingPeer": false,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer2-manufacturer": {
                    "endorsingPeer": false,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer1-middleman": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer2-middleman": {
                    "endorsingPeer": false,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                }
            }
        }

   },

    "organizations": {

        "SupplierOrg": {
            "mspid": "SupplierMSP",
            "peers": [
                "peer1-supplier",
                "peer2-supplier"
            ],
            "certificateAuthorities": [
                "supplier-ca"
            ],
            "adminPrivateKeyPEM": {
                "path": "../network/supplier/admin/msp/keystore/private_sk"
            },
            "signedCertPEM": {
                "path": "../network/supplier/admin/msp/signcerts/cert.pem"
            }
        },
        "CarrierOrg": {
            "mspid": "CarrierMSP",
            "peers": [
                "peer1-carrier",
                "peer2-carrier"
            ],
            "certificateAuthorities": [
                "carrier-ca"
            ],
            "adminPrivateKeyPEM": {
                "path": "../network/carrier/admin/msp/keystore/private_sk"
            },
            "signedCertPEM": {
                "path": "../network/carrier/admin/msp/signcerts/cert.pem"
            }
        },
        "ManufacturerOrg": {
            "mspid": "ManufacturerMSP",
            "peers": [
                "peer1-manufacturer",
                "peer2-manufacturer"
            ],
            "certificateAuthorities": [
                "manufacturer-ca"
            ],
            "adminPrivateKeyPEM": {
                "path": "../network/manufacturer/admin/msp/keystore/private_sk"
            },
            "signedCertPEM": {
                "path": "../network/manufacturer/admin/msp/signcerts/cert.pem"
            }
        },
        "MiddlemanOrg": {
            "mspid": "MiddlemanMSP",
            "peers": [
                "peer1-middleman",
                "peer2-middleman"
            ],
            "certificateAuthorities": [
                "middleman-ca"
            ],
            "adminPrivateKeyPEM": {
                "path": "../network/middleman/admin/msp/keystore/private_sk"
            },
            "signedCertPEM": {
                "path": "../network/middleman/admin/msp/signcerts/cert.pem"
            }
        }
    },
    "orderers": {
        "orderer-cbpm": {
            "url": "grpcs://0.0.0.0:7050",
            "mspid": "OrdererMSP",
            "grpcOptions": {
                "ssl-target-name-override": "orderer-cbpm",
                "hostnameOverride": "orderer-cbpm"
            },
            "tlsCACerts": {
                "path": "../network/cbpm/orderer/tls/tlscacerts/tls-0-0-0-0-7052.pem"
            },
            "adminPrivateKeyPEM": {
                "path": "../network/cbpm/admin/msp/keystore/private_sk"
            },
            "signedCertPEM": {
                "path": "../network/cbpm/admin/msp/signcerts/cert.pem"
            }
        }
    },
    "peers": {
        "peer1-supplier": {
            "url": "grpcs://0.0.0.0:8251",
            "grpcOptions": {
                "ssl-target-name-override": "peer1-supplier",
                "hostnameOverride": "peer1-supplier",
                "request-timeout": 120001
